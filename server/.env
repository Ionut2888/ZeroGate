# Development environment variables for ZeroGate Server
PORT=3001
NODE_ENV=development
JWT_SECRET=your-super-secure-jwt-secret-key-here-change-in-production
JWT_EXPIRES_IN=1h
ZOKRATES_PATH=/usr/local/bin/zokrates
CIRCUITS_PATH=../circuits
CORS_ORIGIN=http://localhost:3000
LOG_LEVEL=info

# Rate Limiting (for login endpoints only)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=20
